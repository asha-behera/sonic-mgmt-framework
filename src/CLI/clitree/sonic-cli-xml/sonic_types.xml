<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE CLISH_MODULE [
<!ENTITY ETH_INTF_TUPLE  "([1-9]([0-9])*)">
<!ENTITY SYSTEM_ID       "((&ETH_INTF_TUPLE;)|(&ETH_INTF_TUPLE;/&ETH_INTF_TUPLE;))">
<!ENTITY ETH_PHY_NUM     "((&ETH_INTF_TUPLE;/&ETH_INTF_TUPLE;/&ETH_INTF_TUPLE;))">
<!ENTITY ETH_PHY_NUM_WITH_SUBPORT    "(&ETH_PHY_NUM;:&ETH_INTF_TUPLE;)">
<!ENTITY ETH_OPTION      "(^e([t]|(th)|(thernet))?\s*)">
<!ENTITY VLAN_OPTION     "(^v([l]|(lan))?\s*)">
<!ENTITY PO_OPTION       "(^p([o]|(ort-channel))?\s*)">
<!ENTITY FC_OPTION       "(^f([c]|(ib)|(ibre-channel))?\s*)">
<!ENTITY ETH_TUPLE_OPTION "(&ETH_PHY_NUM;)|(&ETH_PHY_NUM_WITH_SUBPORT;)">
<!ENTITY ETH_PAIR        "((&ETH_TUPLE_OPTION;)(\-(&ETH_TUPLE_OPTION;))?)">
<!ENTITY ETH_ALL_INTF    "(^e([t]|(th)|(thernet))?\s*((&ETH_PHY_NUM;$)|(&ETH_PHY_NUM_WITH_SUBPORT;$)))">
<!ENTITY VLAN_NUM        "(409[0-3]|40[0-8][0-9]|[1-3][0-9]{3}|[1-9][0-9]{2}|[1-9][0-9]|[1-9])">
<!ENTITY VLAN_PAIR       "((&VLAN_NUM;)(\-(&VLAN_NUM;))?)">
<!ENTITY VLAN_ALL_INTF   "(^v([l]|(lan))?\s*&VLAN_NUM;$)">
<!ENTITY PO_INTF_VALUE   "(12[0-8]|1[0-1][0-9]|[1-9][0-9]|[1-9])">
<!ENTITY PO_PAIR         "((&PO_INTF_VALUE;)(\-(&PO_INTF_VALUE;))?)">
<!ENTITY PO_ALL_INTF     "^p([o]|(ort-channel))?\s*&PO_INTF_VALUE;$">
<!ENTITY FC_ALL_INTF     "(^f([c]|(ib)|(ibre-channel))?\s*((&ETH_PHY_NUM;$)|(&ETH_PHY_NUM_WITH_SUBPORT;$)))">
<!ENTITY LOOPB_INTF_VAL  "([0-9]|[1-9][0-9]|[1-9][0-9]{2}|[1-9][0-9]{3}|1[0-5][0-9]{3}|16[0-2][0-9]{2}|163[0-7][0-9]|1638[0-3])">
<!ENTITY LOOPB_OPTION    "(^l([o]|(oop)|(oopback))?\s*)">
<!ENTITY LOOPB_INTF      "&LOOPB_OPTION;&LOOPB_INTF_VAL;$">
<!ENTITY NULL_OPTION     "(^n([u]|(ull))?\s*)">
<!ENTITY NULL_INTF       "&NULL_OPTION;(0)$">
<!ENTITY MGMT_OPTION     "(^m([g]|(gmt))?\s*)">
<!ENTITY MGMT_INTF       "&MGMT_OPTION;(1/1/1)$">
<!ENTITY VLAN_RNG        "(&VLAN_NUM;-&VLAN_NUM;)">
<!ENTITY ETH_RANGE       "&ETH_OPTION;&ETH_PAIR;(,&ETH_PAIR;)*">
<!ENTITY PO_RANGE        "&PO_OPTION;&PO_PAIR;(,&PO_PAIR;)*">
<!ENTITY VLAN_RANGE      "&VLAN_OPTION;&VLAN_PAIR;(,&VLAN_PAIR;)*">
<!ENTITY TAG_VLAN_RANGE  "&VLAN_PAIR;(,&VLAN_PAIR;)*">
<!ENTITY FC_RANGE       "&FC_OPTION;&ETH_PAIR;(,&ETH_PAIR;)*">
<!ENTITY PORT_NUM        "(6553[0-5]|655[0-2][0-9]|65[0-4][0-9][0-9]|6[0-4][0-9]{3}|[1-5][0-9]{4}|[1-9][0-9]{0,3}|0)">
<!ENTITY IPV4_OPTIONAL_PORT "((((25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.){3}(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)))(:&PORT_NUM;)?">
<!ENTITY IPV6_WITH_PORT     "\[((([0-9a-fA-F]{1,4}:){7,7}[0-9a-fA-F]{1,4}|([0-9a-fA-F]{1,4}:){1,7}:|([0-9a-fA-F]{1,4}:){1,6}:[0-9a-fA-F]{1,4}|([0-9a-fA-F]{1,4}:){1,5}(:[0-9a-fA-F]{1,4}){1,2}|([0-9a-fA-F]{1,4}:){1,4}(:[0-9a-fA-F]{1,4}){1,3}|([0-9a-fA-F]{1,4}:){1,3}(:[0-9a-fA-F]{1,4}){1,4}|([0-9a-fA-F]{1,4}:){1,2}(:[0-9a-fA-F]{1,4}){1,5}|[0-9a-fA-F]{1,4}:((:[0-9a-fA-F]{1,4}){1,6})|:((:[0-9a-fA-F]{1,4}){1,7}|:)))\]:(&PORT_NUM;)">
<!ENTITY IPV4_ADDRESS    "(^(([0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5])\.){3}([0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5])$)">
<!ENTITY UINT_16_RANGE   "(^([0-9]{1,4}|[1-5][0-9]{4}|6[0-4][0-9]{3}|65[0-4][0-9]{2}|655[0-2][0-9]|6553[0-5])$)">
<!ENTITY DSCP_NUM        "(6[0-3]|[1-5][0-9]|[0-9])">
<!ENTITY DSCP_RANGE      "^(&DSCP_NUM;)((,(&DSCP_NUM;)+)*(\-(&DSCP_NUM;)+)*)*$">
<!ENTITY MST_INST_NUM    "(6[0-3]|[1-5][0-9]|[0-9])">
<!ENTITY MST_INST_PAIR       "((&MST_INST_NUM;)(\-(&MST_INST_NUM;))?)">
<!ENTITY MST_INST_NUM_RANGE  "&MST_INST_PAIR;(,&MST_INST_PAIR;)*">
<!ENTITY PRECE_NUM       "([0-7])">
<!ENTITY PRECE_RANGE     "^(&PRECE_NUM;)((,(&PRECE_NUM;)+)*(\-(&PRECE_NUM;)+)*)*$">
<!ENTITY COS_NUM         "([0-7])">
<!ENTITY TC_NUM          "([0-7])">
<!ENTITY COS_RANGE       "^(&COS_NUM;)((,(&COS_NUM;)+)*(\-(&COS_NUM;)+)*)*$">
<!ENTITY TC_RANGE        "^(&TC_NUM;)((,(&TC_NUM;)+)*(\-(&TC_NUM;)+)*)*$">
<!ENTITY RANGE_STR       "(^range)?\s*$)">
<!ENTITY TCP_PORT_RNG    "^(&PORT_NUM;)((,(&PORT_NUM;)+)*(\-(&PORT_NUM;)+)*)*$">
<!ENTITY HOSTNAME_PATTERN    "(^([A-Z]|[a-z]|[0-9]))([A-Z]|[a-z]*|[0-9]|\-)*([^-]$)">
<!ENTITY HOSTNAME_PATTERN_64    "^([a-zA-Z0-9]|[a-zA-Z0-9][a-zA-Z0-9\-]{0,61}[a-zA-Z0-9])(\.([a-zA-Z0-9]|[a-zA-Z0-9][a-zA-Z0-9\-]{0,61}[a-zA-Z0-9]))*$">
]>
<CLISH_MODULE
    xmlns="http://www.dellemc.com/force10/XMLSchema"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:xi="http://www.w3.org/2001/XInclude"
    xsi:schemaLocation="http://www.dellemc.com/force10/XMLSchema
    http://www.dellemc.com/force10/XMLSchema/clish.xsd"
    >
    <!--=======================================================-->
    <PTYPE
        name="STACK"
        method="integer"
        pattern="1..1"
        help=""
        />
    <!--=======================================================-->
    <PTYPE
        name="VLAN_ID"
        method="integer"
        pattern="1..4093"
        help=""
        />
    <!--=======================================================-->
    <PTYPE
        name="MIN_LINK"
        method="integer"
        pattern="1..32"
        help=""
        />
    <!--=======================================================-->
    <PTYPE
        name="RATE_INT"
        method="integer"
        pattern="5..299"
        help=""
        />
    <!--=======================================================-->
    <PTYPE
        name="UNIT_ID"
        method="integer"
        pattern="1..99"
        help=""
        />
    <!--=======================================================-->
    <PTYPE
        name="SWITCH_ID"
        pattern="([1-9]|10)/([1-9]|[1-9][0-9]|1[0-1][0-9]|12[0-8])"
        help="Node/Unit"
        />
    <!--=======================================================-->
    <PTYPE
        name="LAG_NUM"
        method="integer"
        pattern="1..128"
        help=""
        />
    <!--=======================================================-->
    <PTYPE
        name="PHY_ETH_NUM"
        pattern="([1-9]|10)/([1-9]|[1-9][0-9]|1[0-1][0-9]|12[0-8])/([1-9]|[1-9][0-9]|[1-4][0-9][0-9]|50[0-9]|51[0-2])"
        help="Chassis/Slot/Port"
        />
     <!--=======================================================-->
     <PTYPE
        name="PORT_GROUP_ID"
        pattern="([1-9]|10)/([1-9]|[1-9][0-9]|1[0-1][0-9]|12[0-8])/([1-9]|[1-9][0-9]|2[0-7])"
        help="Chassis/Slot/PortGroup(1-28)"
        />
    <!--=======================================================-->
    <PTYPE
        name="SYSTEM_ID"
        pattern="(&SYSTEM_ID;)"
        help="&lt;node-id | node-id/unit-id&gt;"
        />
    <!--=======================================================-->
    <PTYPE
        name="ON_OFF"
        method="select"
        pattern="on(true) off(false)"
        help="on|off"
        />
    <!--=======================================================-->
    <PTYPE
        name="LOOPBACK_NUM"
        method="integer"
        pattern="0..16383"
        help=""
        />
    <!--=======================================================-->
    <PTYPE
        name="NULL_INTF_NUM"
        method="integer"
        pattern="0..0"
        help=""
        />
    <!--=======================================================-->
    <PTYPE
        name="MGMT_INTF_NUM"
        pattern="(1/1/1)"
        help="Chassis/Slot/Port"
        />
    <!--=======================================================-->
    <PTYPE
        name="TRACK_ID"
        method="integer"
        pattern="1..500"
        help=""
        />
    <!--=======================================================-->
    <PTYPE name="AS_NUM_DOT"
        pattern="(^([0-9]{1,4}|[1-5][0-9]{4}|6[0-4][0-9]{3}|65[0-4][0-9]{2}|655[0-2][0-9]|6553[0-5])[.]([0-9]{1,4}|[1-5][0-9]{4}|6[0-4][0-9]{3}|65[0-4][0-9]{2}|655[0-2][0-9]|6553[0-5])$)|^([1-9][0-9]{0,8})$|^([1-3][0-9]{9})$|^(4[0-1][0-9]{8}$)|^(42[0-8][0-9]{7})$|^(429[0-3][0-9]{6})$|^(4294[0-8][0-9]{5})$|^(42949[0-5][0-9]{4})$|^(429496[0-6][0-9]{3})$|^(4294967[0-1][0-9]{2})$|^(42949672[0-8][0-9])$|^(429496729[0-5])$"
        help="0.1..65535.65535 or 1..4294967295"/>
    <!--=======================================================-->
    <PTYPE
        name="AA_NN"
        pattern="(^([0-9]{1,4}|[1-5][0-9]{4}|6[0-4][0-9]{3}|65[0-4][0-9]{2}|655[0-2][0-9]|6553[0-5])[:]([0-9]{1,4}|[1-5][0-9]{4}|6[0-4][0-9]{3}|65[0-4][0-9]{2}|655[0-2][0-9]|6553[0-5])$)"
        help="0..65535:0..65535"
        />
    <!--=======================================================-->
    <PTYPE
        name="ASN2_NN"
        pattern="(^([0-9]{1,4}|[1-5][0-9]{4}|6[0-4][0-9]{3}|65[0-4][0-9]{2}|655[0-2][0-9]|6553[0-5])[:](([1-9][0-9]{0,8})|([1-3][0-9]{9})|(4[0-1][0-9]{8}$)|(42[0-8][0-9]{7})|(429[0-3][0-9]{6})|(4294[0-8][0-9]{5})|(42949[0-5][0-9]{4})|(429496[0-6][0-9]{3})|(4294967[0-1][0-9]{2})|(42949672[0-8][0-9])|(429496729[0-5]{1}))$)"
        help="1..65535:1..4294967295"
        />
    <!--=======================================================-->
    <PTYPE
        name="ASN4_NN"
        pattern="((^(([1-9][0-9]{0,8})|([1-3][0-9]{9})|(4[0-1][0-9]{8})|(42[0-8][0-9]{7})|(429[0-3][0-9]{6})|(4294[0-8][0-9]{5})|(42949[0-5][0-9]{4})|(429496[0-6][0-9]{3})|(4294967[0-1][0-9]{2})|(42949672[0-8][0-9])|(429496729[0-5]))[:]([0-9]{1,4}|[1-5][0-9]{4}|6[0-4][0-9]{3}|65[0-4][0-9]{2}|655[0-2][0-9]|6553[0-5])$)|(^([0-9]{1,4}|[1-5][0-9]{4}|6[0-4][0-9]{3}|65[0-4][0-9]{2}|655[0-2][0-9]|6553[0-5])[.]([0-9]{1,4}|[1-5][0-9]{4}|6[0-4][0-9]{3}|65[0-4][0-9]{2}|655[0-2][0-9]|6553[0-5])[:]([0-9]{1,4}|[1-5][0-9]{4}|6[0-4][0-9]{3}|65[0-4][0-9]{2}|655[0-2][0-9]|6553[0-5])$))"
        help="1..4294967295:1..65535 or 1..65535.1..65535:1..65535"
        />
    <!--=======================================================-->
    <PTYPE
        name="IPADDR_NN"
        pattern="(^(([0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5])\.){3}([0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5])[:]([0-9]{1,4}|[1-5][0-9]{4}|6[0-4][0-9]{3}|65[0-4][0-9]{2}|655[0-2][0-9]|6553[0-5])$)"
        help="A.B.C.D:[1..65535]"
        />
    <!--=======================================================-->
    <PTYPE
        name="INT_OR_IP_ADDR"
        pattern="(&IPV4_ADDRESS;|&UINT_16_RANGE;)"
        help="A.B.C.D or [0..65535]"
        />
    <!--=======================================================-->
    <PTYPE
        name="IP_ADDR"
        pattern="(&IPV4_ADDRESS;)"
        help="A.B.C.D"
        />
    <!--=======================================================-->
    <PTYPE
        name="IPV4_ADDR_ABC"
        pattern="(([1-9]|[1-9][0-9]|1[0-9][0-9]|2[0-2][0-3])(\.([2][0-5][0-5]|[1][0-9][0-9]|[1-9][0-9]|[0-9])){3})"
        help="A.B.C.D"
        />
    <!--=======================================================-->
    <PTYPE
        name="IPV4V6_ADDR"
        pattern="((((25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.){3}(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?))|(([^:]+:){6}(([^:]+:[^:]+)|(.*\..*)))|((([^:]+:)*[^:]+)?::(([^:]+:)*[^:]+)?)(%.+)?)"
        help="A.B.C.D/A::B"
        />
    <!--=======================================================-->
    <PTYPE
        name="IPV6_ADDR"
        pattern="(([0-9a-fA-F]{1,4}:){7,7}[0-9a-fA-F]{1,4}|([0-9a-fA-F]{1,4}:){1,7}:|([0-9a-fA-F]{1,4}:){1,6}:[0-9a-fA-F]{1,4}|([0-9a-fA-F]{1,4}:){1,5}(:[0-9a-fA-F]{1,4}){1,2}|([0-9a-fA-F]{1,4}:){1,4}(:[0-9a-fA-F]{1,4}){1,3}|([0-9a-fA-F]{1,4}:){1,3}(:[0-9a-fA-F]{1,4}){1,4}|([0-9a-fA-F]{1,4}:){1,2}(:[0-9a-fA-F]{1,4}){1,5}|[0-9a-fA-F]{1,4}:((:[0-9a-fA-F]{1,4}){1,6})|:((:[0-9a-fA-F]{1,4}){1,7}|:))$"
        help="A::B"
        />
    <!--=======================================================-->
    <PTYPE
        name="IPV6_ADDR_MASK"
        pattern="(([0-9a-fA-F]{1,4}:){7,7}[0-9a-fA-F]{1,4}|([0-9a-fA-F]{1,4}:){1,7}:|([0-9a-fA-F]{1,4}:){1,6}:[0-9a-fA-F]{1,4}|([0-9a-fA-F]{1,4}:){1,5}(:[0-9a-fA-F]{1,4}){1,2}|([0-9a-fA-F]{1,4}:){1,4}(:[0-9a-fA-F]{1,4}){1,3}|([0-9a-fA-F]{1,4}:){1,3}(:[0-9a-fA-F]{1,4}){1,4}|([0-9a-fA-F]{1,4}:){1,2}(:[0-9a-fA-F]{1,4}){1,5}|[0-9a-fA-F]{1,4}:((:[0-9a-fA-F]{1,4}){1,6})|:((:[0-9a-fA-F]{1,4}){1,7}|:))(\/([0-9]|[1-9][0-9]|1[0-1][0-9]|12[0-8]))$"
        help="A::B/mask"
        preprocess="tolower"
        />
    <!--=======================================================-->
    <PTYPE
        name="IPV4_OR_IPV6_ADDR"
        pattern="(^(((25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.){3}(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?))$)|(^(([0-9a-fA-F]{1,4}:){7,7}[0-9a-fA-F]{1,4}|([0-9a-fA-F]{1,4}:){1,7}:|([0-9a-fA-F]{1,4}:){1,6}:[0-9a-fA-F]{1,4}|([0-9a-fA-F]{1,4}:){1,5}(:[0-9a-fA-F]{1,4}){1,2}|([0-9a-fA-F]{1,4}:){1,4}(:[0-9a-fA-F]{1,4}){1,3}|([0-9a-fA-F]{1,4}:){1,3}(:[0-9a-fA-F]{1,4}){1,4}|([0-9a-fA-F]{1,4}:){1,2}(:[0-9a-fA-F]{1,4}){1,5}|[0-9a-fA-F]{1,4}:((:[0-9a-fA-F]{1,4}){1,6})|:((:[0-9a-fA-F]{1,4}){1,7}|:))$)"
        help="A.B.C.D or A:B:C:D:E:F:G:H"
        />
    <!--=======================================================-->
    <PTYPE
        name="HOSTNAME_OR_IPADDR"
        pattern="(^([a-zA-Z0-9]([a-zA-Z0-9\-]{0,61}[a-zA-Z0-9])?\.?)+[a-zA-Z0-9]{2,6}$)|(^(((25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.){3}(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?))$)|(^(([0-9a-fA-F]{1,4}:){7,7}[0-9a-fA-F]{1,4}|([0-9a-fA-F]{1,4}:){1,7}:|([0-9a-fA-F]{1,4}:){1,6}:[0-9a-fA-F]{1,4}|([0-9a-fA-F]{1,4}:){1,5}(:[0-9a-fA-F]{1,4}){1,2}|([0-9a-fA-F]{1,4}:){1,4}(:[0-9a-fA-F]{1,4}){1,3}|([0-9a-fA-F]{1,4}:){1,3}(:[0-9a-fA-F]{1,4}){1,4}|([0-9a-fA-F]{1,4}:){1,2}(:[0-9a-fA-F]{1,4}){1,5}|[0-9a-fA-F]{1,4}:((:[0-9a-fA-F]{1,4}){1,6})|:((:[0-9a-fA-F]{1,4}){1,7}|:))$)"
        help="Hostname or IPv4/v6 address"
        />
    <!--=======================================================-->
    <PTYPE
        name="HOSTNAME_OR_IPV4_ADDR"
        pattern="(^([a-zA-Z0-9]([a-zA-Z0-9\-]{0,61}[a-zA-Z0-9])?\.?)+[a-zA-Z0-9]{2,6}$)|(^(((25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.){3}(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?))$)"
        help="Hostname or IPv4 address"
        />
    <!--=======================================================-->
    <PTYPE
        name="HOSTNAME_STR"
        pattern="(&HOSTNAME_PATTERN;)"
        help="Hostname string"
        />
    <!--=======================================================-->
    <PTYPE
        name="HOSTNAME_STR_64"
        pattern="(&HOSTNAME_PATTERN_64;)"
        help="Hostname string(Max: 64 characters)"
        />
    <!--=======================================================-->
    <PTYPE
        name="EMAILID"
        pattern="^([A-Za-z0-9][A-Za-z0-9._%+-]{1,64}@([A-Za-z0-9-]{1,63}\.){1,8}[A-Za-z]{2,63})$"
        help="Email address"
        />
    <!--=======================================================-->
    <PTYPE
        name="UINT16"
        method="integer"
        pattern="0..65535"
        help=""
        />
    <!--=======================================================-->
    <PTYPE
        name="UINT"
        pattern="[0-9]+"
        help="Unsigned integer"
        />
    <!--=======================================================-->
    <PTYPE
        name="STRING"
        pattern="[^|]+"
        help="String"
        />
    <!--=======================================================-->
    <PTYPE
        name="PASSWORD_STR"
        pattern="[^,]+"
        help="String"
        />
    <!--=======================================================-->
    <PTYPE
        name="STRING_8"
        pattern="(^([a-zA-Z]){1,8})$"
        help="String"
        />
    <!--=======================================================-->
    <PTYPE
        name="STRING_20"
        pattern="(^([a-zA-Z0-9_-]){1,20})$"
        help="String"
        />
    <!--=======================================================-->
    <PTYPE
        name="STRING_32"
        pattern="(^([a-zA-Z0-9_-]){0,32})$"
        help="String(Max: 32 characters)"
        />
    <!--=======================================================-->
    <PTYPE
        name="STRING_55"
        pattern="(^([a-zA-Z0-9 _-]){0,55})$"
        help="String"
        />
    <!--=======================================================-->
    <PTYPE
        name="STRING_WITH_PIPE"
        pattern=".+"
        help="String"
        />
    <!--=======================================================-->
     <PTYPE
        name="URL"
        pattern="[^|]+"
        help="Enter the string URL in one of the following formats:"
        />
    <!--=======================================================-->
    <PTYPE
        name="SUBCOMMAND"
        pattern="[^\]+"
        help="String"
        />
    <!--=======================================================-->
    <PTYPE
        name="BOOL"
        method="select"
        pattern="true(1) false(0)"
        help="Boolean choice"
        />
    <!--=======================================================-->
    <PTYPE
        name="IFACE_NUM"
        pattern="(&ETH_PHY_NUM;|&ETH_PHY_NUM_WITH_SUBPORT;)"
        help="&lt;chassis/slot/port[:subport]&gt;"
        />
    <!--=======================================================-->
    <PTYPE
