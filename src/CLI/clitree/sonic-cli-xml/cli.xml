<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE CLISH_MODULE [
<!ENTITY MODULE-NS      "http://www.dellemc.com/networking/os10/dell-cli">
<!ENTITY FEATURES_LIST    "/cli-config/features">
<!ENTITY FEATURE_NAME     "/cli-config/features/name">
<!ENTITY FEATURE_ENABLE   "/cli-config/features/enable">
<!ENTITY CLI_CONFIG_ALIAS       "/cli-config/alias">
<!ENTITY CLI_CONFIG_ALIAS_NAME  "&CLI_CONFIG_ALIAS;/name">
<!ENTITY CLI_CONFIG_ALIAS_VALUE  "&CLI_CONFIG_ALIAS;/value">
]>
<CLISH_MODULE
    xmlns="http://www.dellemc.com/force10/XMLSchema"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:xi="http://www.w3.org/2001/XInclude"
    xsi:schemaLocation="http://www.dellemc.com/force10/XMLSchema
                        http://www.dellemc.com/force10/XMLSchema/clish.xsd"
    >
<!--=========================================================================-->

<VIEW name="enable-view">
    <!-- alias <alias-name> <alias-value> -->
    <COMMAND name="alias"
        help="Set alias for a command"
        prompt_fn="configure_local_alias"
        prompt_oper="${alias-name} ${alias-value}">
        <PARAM name="alias-name"
            help="Name of the alias (Max size 20)"
            ptype="STRING_20">
            <PARAM name="alias-value"
                help="Value of the alias"
                ptype="STRING_WITH_PIPE"/>
        </PARAM>
        <!--ACTION builtin="clish_extn"></ACTION-->
    </COMMAND>

    <!-- no alias <alias-name> -->
    <COMMAND name="no alias"
        help="Remove an alias definition">
        <PARAM name="alias-name"
            help="Name of the alias (Max size 20)"
            ptype="STRING_20">
        </PARAM>
        <!--ACTION builtin="clish_no_alias_plugin">${alias-name}</ACTION-->
    </COMMAND>

    <!-- show alias -->
    <COMMAND name="show alias"
         help="Show list of aliases">

        <PARAM name="show-options"
            help="show options"
            ptype="SUBCOMMAND"
            mode="switch"
            optional="true"
            >
            <PARAM name="brief"
                help="Brief information of aliases"
                ptype="SUBCOMMAND"
                mode="subcommand"
            />
            <PARAM name="detail"
                help="Detail information of aliases"
                ptype="SUBCOMMAND"
                mode="subcommand"
            />
        </PARAM>

         <!--ACTION builtin="clish_show_alias_plugin"/-->

    </COMMAND>
</VIEW>

<VIEW name="configure-view">

    <!-- feature <feature-name> -->
    <COMMAND name="feature"
        help="Enable feature"
        operation="edit-config"
        namespace="&MODULE-NS;"
        yang_key="&FEATURE_NAME;"
        >

        <PARAM name="feature-name-options"
            help="feature names"
            ptype="SUBCOMMAND"
            mode="switch"
            >
            <PARAM name="config-os9-style"
                help="Enable OS9 style configuration commands"
                ptype="SUBCOMMAND"
                mode="subcommand"
                yang_name="&FEATURE_NAME;=config-os9-style,&FEATURE_ENABLE;=true"
            />
        </PARAM>

        <ACTION builtin="clish_nop"> </ACTION>

    </COMMAND>

    <!-- no feature <feature-name> -->
    <COMMAND name="no feature"
        help="Disable feature"
        operation="edit-config"
        namespace="&MODULE-NS;"
        >

        <PARAM name="feature-name-options"
            help="feature names"
            ptype="SUBCOMMAND"
            mode="switch"
            >
            <PARAM name="config-os9-style"
                help="Disable OS9 style configuration commands"
                ptype="SUBCOMMAND"
                mode="subcommand"
                yang_name="&FEATURE_NAME;=config-os9-style,&FEATURE_ENABLE;=true"
                sub_oper_yang="remove:enable"
            />
        </PARAM>

        <ACTION builtin="clish_nop"> </ACTION>

    </COMMAND>

    <!-- alias <alias-name> <alias-value> -->
    <COMMAND name="alias"
        help="Set alias for a command"
        operation="edit-config"
        namespace="http://www.dellemc.com/networking/os10/dell-cli"
        yang_key="&CLI_CONFIG_ALIAS_NAME;"
        prompt_fn="configure_alias"
        prompt_oper="${alias-name} ${alias-value}">
        <PARAM name="alias-name"
            help="Name of the alias (Max size 20)"
            ptype="STRING_20"
            yang_name="&CLI_CONFIG_ALIAS_NAME;=${alias-name}">
            <PARAM name="alias-value"
                help="Value of the alias"
                ptype="STRING_WITH_PIPE"
                yang_name="&CLI_CONFIG_ALIAS_VALUE;"/>
        </PARAM>
        <ACTION builtin="clish_nop"></ACTION>
    </COMMAND>
    <!-- no alias <alias-name> -->
    <COMMAND name="no alias"
        help="Remove an alias definition"
        operation="edit-config"
        namespace="http://www.dellemc.com/networking/os10/dell-cli">
        <PARAM name="alias-name"
            help="Name of the alias (Max size 20)"
            ptype="STRING_20"
            yang_name="&CLI_CONFIG_ALIAS_NAME;=${alias-name}"
            sub_oper_yang="remove:alias">
        </PARAM>
        <ACTION builtin="clish_nop"></ACTION>
    </COMMAND>

</VIEW>

</CLISH_MODULE>
