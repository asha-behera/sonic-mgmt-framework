<ROOT>
<MACRODEF name = "MACRO_LLDP_BASIC_TLV" >
    <PARAM
        name="basic-tlv"
        help="Enable basic tlvs"
        ptype="SUBCOMMAND"
        mode="subcommand"
        >
        <PARAM
            name="basic-tlv-subcmds"
            help="sub-commands"
            ptype="SUBCOMMAND"
            mode="switch"
            >
            <PARAM
                name="management-address"
                help="Enable management address tlv"
                ptype="SUBCOMMAND"
                mode="subcommand"
                sub_oper_yang="arg1:mgmt-address-enable"
                yang_name="&LLDP_INTF_NAME;,&LLDP_BASIC_TLVS;/dell-lldp:mgmt-address-enable=arg2"
                />
            <PARAM
                name="port-description"
                help="Enable port description tlv"
                ptype="SUBCOMMAND"
                mode="subcommand"
                sub_oper_yang="arg1:port-desc-enable"
                yang_name="&LLDP_INTF_NAME;,&LLDP_BASIC_TLVS;/dell-lldp:port-desc-enable=arg2"
                />
            <PARAM
                name="system-capabilities"
                help="Enable system capabilities tlv"
                ptype="SUBCOMMAND"
                mode="subcommand"
                sub_oper_yang="arg1:sys-cap-enable"
                yang_name="&LLDP_INTF_NAME;,&LLDP_BASIC_TLVS;/dell-lldp:sys-cap-enable=arg2"
                />
            <PARAM
                name="system-description"
                help="Enable system description tlv"
                ptype="SUBCOMMAND"
                mode="subcommand"
                sub_oper_yang="arg1:sys-desc-enable"
                yang_name="&LLDP_INTF_NAME;,&LLDP_BASIC_TLVS;/dell-lldp:sys-desc-enable=arg2"
                />
            <PARAM
                name="system-name"
                help="Enable system name tlv"
                ptype="SUBCOMMAND"
                mode="subcommand"
                sub_oper_yang="arg1:sys-name-enable"
                yang_name="&LLDP_INTF_NAME;,&LLDP_BASIC_TLVS;/dell-lldp:sys-name-enable=arg2"
                />
        </PARAM>
        <PARAM
            name="management-address"
            test="'${basic-tlv-subcmds}' != 'management-address'"
            help="Enable management address tlv"
            ptype="SUBCOMMAND"
            mode="subcommand"
            optional="true"
            db_flag="SKIP_TREE"
            sub_oper_yang="arg1:mgmt-address-enable"
            yang_name="&LLDP_BASIC_TLVS;/dell-lldp:mgmt-address-enable=arg2"
            />
        <PARAM
            name="port-description"
            test="'${basic-tlv-subcmds}' != 'port-description'"
            help="Enable port description tlv"
            ptype="SUBCOMMAND"
            mode="subcommand"
            optional="true"
            db_flag="SKIP_TREE"
            sub_oper_yang="arg1:port-desc-enable"
            yang_name="&LLDP_BASIC_TLVS;/dell-lldp:port-desc-enable=arg2"
            />
        <PARAM
            name="system-capabilities"
            test="'${basic-tlv-subcmds}' != 'system-capabilities'"
            help="Enable system capabilities tlv"
            ptype="SUBCOMMAND"
            mode="subcommand"
            optional="true"
            db_flag="SKIP_TREE"
            sub_oper_yang="arg1:sys-cap-enable"
            yang_name="&LLDP_BASIC_TLVS;/dell-lldp:sys-cap-enable=arg2"
            />
        <PARAM
            name="system-description"
            test="'${basic-tlv-subcmds}' != 'system-description'"
            help="Enable system description tlv"
            ptype="SUBCOMMAND"
            mode="subcommand"
            optional="true"
            db_flag="SKIP_TREE"
            sub_oper_yang="arg1:sys-desc-enable"
            yang_name="&LLDP_BASIC_TLVS;/dell-lldp:sys-desc-enable=arg2"
            />
        <PARAM
            name="system-name"
            test="'${basic-tlv-subcmds}' != 'system-name'"
            help="Enable system name tlv"
            ptype="SUBCOMMAND"
            mode="subcommand"
            optional="true"
            db_flag="SKIP_TREE"
            sub_oper_yang="arg1:sys-name-enable"
            yang_name="&LLDP_BASIC_TLVS;/dell-lldp:sys-name-enable=arg2"
            />
    </PARAM>
</MACRODEF>

<MACRODEF name = "MACRO_LLDP_DOT1_TLV" >
    <PARAM
        name="dot1tlv"
        help="Enable dot1 tlvs"
        ptype="SUBCOMMAND"
        mode="subcommand"
        >
        <PARAM
            name="dot1tlv-subcmds"
            help="sub-commands"
            ptype="SUBCOMMAND"
            mode="switch"
            >
            <!-- LLDP version 2009 sends link aggregation TLV as a Dot1 TLV. So CLI also needs to be consistent -->
            <PARAM
                name="link-aggregation"
                help="Enable link-aggregation tlv"
                ptype="SUBCOMMAND"
                mode="subcommand"
                sub_oper_yang="arg1:linkagg-enable"
                yang_name="&LLDP_INTF_NAME;,&LLDP_DOT3_TLVS;/dell-lldp:linkagg-enable=arg2"
                />
           <PARAM
                name="port-vlan-id"
                help="Enable port-vlan tlv"
                ptype="SUBCOMMAND"
                mode="subcommand"
                sub_oper_yang="arg1:portvlan-enable"
                yang_name="&LLDP_INTF_NAME;,&LLDP_DOT1_TLVS;/dell-lldp:portvlan-enable=arg2"
                />
        </PARAM>
        <PARAM
            name="link-aggregation"
            test="'${dot1tlv-subcmds}' != 'link-aggregation'"
            help="Enable link-aggregation tlv"
            ptype="SUBCOMMAND"
            mode="subcommand"
            optional="true"
            db_flag="SKIP_TREE"
            sub_oper_yang="arg1:linkagg-enable"
            yang_name="&LLDP_DOT3_TLVS;/dell-lldp:linkagg-enable=arg2"
            />
        <PARAM
            name="port-vlan-id"
            test="'${dot1tlv-subcmds}' != 'port-vlan-id'"
            help="Enable port-vlan tlv"
            ptype="SUBCOMMAND"
            mode="subcommand"
            optional="true"
            db_flag="SKIP_TREE"
            sub_oper_yang="arg1:portvlan-enable"
            yang_name="&LLDP_DOT1_TLVS;/dell-lldp:portvlan-enable=arg2"
            />
    </PARAM>
</MACRODEF>

<MACRODEF name = "MACRO_LLDP_DOT3_TLV" >
    <PARAM
        name="dot3tlv"
        help="Enable dot3 tlvs"
        ptype="SUBCOMMAND"
        mode="subcommand"
        >
        <PARAM
            name="dot3tlv-subcmds"
            help="sub-commands"
            ptype="SUBCOMMAND"
            mode="switch"
            >
            <PARAM
                name="macphy-config"
                help="Enable MAC/PHY configuration tlv"
                ptype="SUBCOMMAND"
                mode="subcommand"
                sub_oper_yang="arg1:mac-phy-config-enable"
                yang_name="&LLDP_INTF_NAME;,&LLDP_DOT3_TLVS;/dell-lldp:mac-phy-config-enable=arg2"
                />
            <PARAM
                name="max-framesize"
                help="Enable maximum frame size tlv"
                ptype="SUBCOMMAND"
                mode="subcommand"
                sub_oper_yang="arg1:max-frame-size-enable"
                yang_name="&LLDP_INTF_NAME;,&LLDP_DOT3_TLVS;/dell-lldp:max-frame-size-enable=arg2"
                />
        </PARAM>
        <PARAM
            name="macphy-config"
            test="'${dot3tlv-subcmds}' != 'macphy-config'"
            help="Enable MAC/PHY configuration tlv"
            ptype="SUBCOMMAND"
            mode="subcommand"
            optional="true"
            db_flag="SKIP_TREE"
            sub_oper_yang="arg1:mac-phy-config-enable"
            yang_name="&LLDP_DOT3_TLVS;/dell-lldp:mac-phy-config-enable=arg2"
            />
        <PARAM
            name="max-framesize"
            test="'${dot3tlv-subcmds}' != 'max-framesize'"
            help="Enable maximum frame size tlv"
            ptype="SUBCOMMAND"
            mode="subcommand"
            optional="true"
            db_flag="SKIP_TREE"
            sub_oper_yang="arg1:max-frame-size-enable"
            yang_name="&LLDP_DOT3_TLVS;/dell-lldp:max-frame-size-enable=arg2"
            />
    </PARAM>
</MACRODEF>
<MACRODEF name = "MACRO_LLDP_APP_TLV" >
    <PARAM
        name="dcbxp-appln"
        help="Enable application tlvs"
        ptype="SUBCOMMAND"
        mode="subcommand"
        >
            <PARAM
                name="iscsi"
                help="Enable iscsi application tlv"
                ptype="SUBCOMMAND"
                mode="subcommand"
                yang_name="&LLDP_INTF_NAME;,&LLDP_APP_TLVS;/dell-iscsi-optimization:appln-tlv-status=arg1"
                />
    </PARAM>
</MACRODEF>

<MACRODEF name = "MACRO_LLDP_MED_ENABLE" >
    <PARAM
        name="disable"
        help="Disables LLDP MED on the interface"
        ptype="SUBCOMMAND"
        mode="subcommand"
        yang_name="&LLDP_MED_INTF;/name=${ifacename},&LLDP_MED_PORT_CFG;/dell-lldp-med:med-enable=false"
        >
    </PARAM>
    <PARAM
        name="enable"
        help="Enables LLDP MED on the interface"
        ptype="SUBCOMMAND"
        mode="subcommand"
        yang_name="&LLDP_MED_INTF;/name=${ifacename},&LLDP_MED_PORT_CFG;/dell-lldp-med:med-enable=true"
        >
    </PARAM>
</MACRODEF>
<MACRODEF name = "MACRO_LLDP_MED_TLV_SELECT" >
    <PARAM
        name="tlv-select"
        help="Specify which LLDP MED TLVs are included"
        ptype="SUBCOMMAND"
        mode="subcommand"
        >
        <PARAM
            name="tlv-select-subcmds"
            help="sub-commands"
            ptype="SUBCOMMAND"
            mode="switch"
            >
            <PARAM
                name="inventory"
                help="Enable Inventory TLV"
                ptype="SUBCOMMAND"
                mode="subcommand"
                yang_name="&LLDP_MED_INTF;/name=${ifacename},&LLDP_MED_PORT_CFG;/dell-lldp-med:tlvs-tx-enable/dell-lldp-med:inventory=true"
                />
            <PARAM
                name="network-policy"
                help="Enable Network policy TLV"
                ptype="SUBCOMMAND"
                mode="subcommand"
                yang_name="&LLDP_MED_INTF;/name=${ifacename},&LLDP_MED_PORT_CFG;/dell-lldp-med:tlvs-tx-enable/dell-lldp-med:network-policy=true"
                />
        </PARAM>
        <PARAM
             name="inventory"
             test="'${tlv-select-subcmds}' != 'inventory'"
             help="Enable Inventory TLV"
             ptype="SUBCOMMAND"
             mode="subcommand"
             optional="true"
             db_flag="SKIP_NODE"
             yang_name="&LLDP_MED_PORT_CFG;/dell-lldp-med:tlvs-tx-enable/dell-lldp-med:inventory=true"
             />
        <PARAM
             name="network-policy"
             test="'${tlv-select-subcmds}' != 'network-policy'"
             help="Enable Network policy TLV"
             ptype="SUBCOMMAND"
             mode="subcommand"
             optional="true"
             db_flag="SKIP_NODE"
             yang_name="&LLDP_MED_PORT_CFG;/dell-lldp-med:tlvs-tx-enable/dell-lldp-med:network-policy=true"
             />
    </PARAM>
</MACRODEF>
<MACRODEF name = "MACRO_LLDP_MED_INTF_NW_POLICY" >
    <PARAM
        name="network-policy"
        help="Attach (or remove) an LLDP MED network policy"
        ptype="SUBCOMMAND"
        mode="subcommand"
        >
        <PARAM
            name="network-policy-subcmds"
            help="sub-commands"
            ptype="SUBCOMMAND"
            mode="switch"
            >
            <PARAM
                name="add"
                help="Attaches the specified network policy"
                ptype="SUBCOMMAND"
                mode="subcommand"
                sub_oper_yang="merge:policy-id"
                add_config_entry="true"
                >
                <PARAM
                    name="value"
                    help="Network policy id"
                    ptype="RANGE_1_32"
                    yang_name="&LLDP_MED_INTF;/name=${ifacename},&LLDP_MED_PORT_CFG;/dell-lldp-med:policy-id=${value}"
                 />
            </PARAM>
            <PARAM
                name="remove"
                help="Removes the specified network policy"
                ptype="SUBCOMMAND"
                mode="subcommand"
                sub_oper_yang="remove:policy-id"
                db_flag="SKIP_TREE"
                >
                <PARAM
                    name="value"
                    help="Network policy id"
                    ptype="RANGE_1_32"
                    yang_name="&LLDP_MED_INTF;/name=${ifacename},&LLDP_MED_PORT_CFG;/dell-lldp-med:policy-id=${value}"
                 />
            </PARAM>
        </PARAM>
    </PARAM>
</MACRODEF>
<MACRODEF name = "MACRO_LLDP_MED_NW_POLICY_END">
    <PARAM
        name="priority"
        help="Specify priority"
        ptype="SUBCOMMAND"
        mode="subcommand"
        namespace="&LLDP_MED_GLOB_CFG_NAMESPACE;"
        >
        <PARAM
            name="pr-value"
            help="Priority value"
            ptype="RANGE_0_7"
            yang_name="&LLDP_MED_POLICY_CFG;/priority=${pr-value}"
            />
        <PARAM
            name="dscp"
            help="Specify DSCP priority value"
            ptype="SUBCOMMAND"
            mode="subcommand"
            >
            <PARAM
                name="dscp-value"
                help="DSCP priority value"
                ptype="RANGE_0_63"
                yang_name="&LLDP_MED_POLICY_CFG;/dscp=${dscp-value}"
                />
        </PARAM>
    </PARAM>
</MACRODEF>
<MACRODEF name = "MACRO_LLDP_MED_NW_POLICY_VLAN">
    <PARAM
        name="vlan"
        help="Specify vlan"
        ptype="SUBCOMMAND"
        mode="subcommand"
        namespace="&LLDP_MED_NAMESPACE;"
        >
        <PARAM
            name="vlan-id"
            help="Vlan ID"
            ptype="VLAN_ID"
            yang_name="&LLDP_MED_POLICY_CFG;/vlan-id=${vlan-id}"
            />
        <PARAM
            name="vlan-type"
            help="Specify vlan type"
            ptype="SUBCOMMAND"
            mode="subcommand"
            >
            <PARAM
                name="vlan-type-subcmds"
                help="sub-commands"
                ptype="SUBCOMMAND"
                mode="switch"
                >
                <PARAM
                    name="tag"
                    help="Tagged vlan"
                    ptype="SUBCOMMAND"
                    mode="subcommand"
                    yang_name="&LLDP_MED_POLICY_CFG;/tagged=true"
                    >
                    <MACRO name="MACRO_LLDP_MED_NW_POLICY_END"/>
                </PARAM>
                <PARAM
                    name="untag"
                    help="Untagged vlan"
                    ptype="SUBCOMMAND"
                    mode="subcommand"
                    yang_name="&LLDP_MED_POLICY_CFG;/tagged=false"
                    >
                    <MACRO name="MACRO_LLDP_MED_NW_POLICY_END"/>
                </PARAM>
             </PARAM>
         </PARAM>
     </PARAM>
</MACRODEF>
<MACRODEF name = "MACRO_LLDP_MED_NW_POLICY" >
    <PARAM
        name="network-policy"
        help="Define an LLDP MED network policy"
        ptype="SUBCOMMAND"
        mode="subcommand"
        namespace="&LLDP_MED_NAMESPACE;"
        >
        <PARAM
            name="value"
            help="Policy ID"
            ptype="RANGE_1_32"
            yang_name="&LLDP_MED_POLICY_CFG;/policy-id=${value}"
            yang_key="&LLDP_MED_POLICY_CFG;/policy-id"
            />
             <PARAM
                 name="app"
                 help="Type of the application defined for this network policy"
                 ptype="SUBCOMMAND"
                 mode="subcommand"
                 >
                <PARAM
                    name="app-subcmds"
                    help="sub-commands"
                    ptype="SUBCOMMAND"
                    mode="switch"
                    >
                    <PARAM
                        name="guest-voice"
                        help="Guest voice application"
                        ptype="SUBCOMMAND"
                        mode="subcommand"
                        yang_name="&LLDP_MED_POLICY_CFG;/app-type=guest-voice"
                        />
                    <PARAM
                        name="guestvoice-signaling"
                        help="Guest voice signaling application"
                        ptype="SUBCOMMAND"
                        mode="subcommand"
                        yang_name="&LLDP_MED_POLICY_CFG;/app-type=guest-voice-signaling"
                        />
                    <PARAM
                        name="softphone-voice"
                        help="Softphone voice application"
                        ptype="SUBCOMMAND"
                        mode="subcommand"
                        yang_name="&LLDP_MED_POLICY_CFG;/app-type=soft-phone-voice"
                        />
                    <PARAM
                        name="streaming-video"
                        help="Streaming video application"
                        ptype="SUBCOMMAND"
                        mode="subcommand"
                        yang_name="&LLDP_MED_POLICY_CFG;/app-type=streaming-video"
                        />
                    <PARAM
                        name="video-conferencing"
                        help="Video conferencing application"
                        ptype="SUBCOMMAND"
                        mode="subcommand"
                        yang_name="&LLDP_MED_POLICY_CFG;/app-type=video-conferencing"
                        />
                    <PARAM
                        name="voice-signaling"
                        help="Voice signaling application"
                        ptype="SUBCOMMAND"
                        mode="subcommand"
                        yang_name="&LLDP_MED_POLICY_CFG;/app-type=voice-signaling"
                        />
                    <PARAM
                        name="voice"
                        help="Voice application"
                        ptype="SUBCOMMAND"
                        mode="subcommand"
                        yang_name="&LLDP_MED_POLICY_CFG;/app-type=voice"
                        />
                    <PARAM
                        name="video-signaling"
                        help="Video signaling application"
                        ptype="SUBCOMMAND"
                        mode="subcommand"
                        yang_name="&LLDP_MED_POLICY_CFG;/app-type=video-signaling"
                        />
                </PARAM>
            </PARAM>
            <MACRO name = "MACRO_LLDP_MED_NW_POLICY_VLAN"/>
      </PARAM>
</MACRODEF>
<!-- INTERFACE RANGE CONFIGS  -->
<MACRODEF name = "MACRO_RANGE_LLDP_BASIC_TLV" >
    <PARAM
        name="basic-tlv"
        help="Enable basic tlvs"
        ptype="SUBCOMMAND"
        mode="subcommand"
