        <PARAM
            name="basic-tlv-subcmds"
            help="sub-commands"
            ptype="SUBCOMMAND"
            mode="switch"
            db_flag="SKIP_TREE"
            >
            <PARAM
                name="management-address"
                help="Enable management address tlv"
                ptype="SUBCOMMAND"
                mode="subcommand"
                sub_oper_yang="arg1:mgmt-address-enable"
                yang_name="&LLDP_RANGE_INTF_NAME;,&LLDP_RANGE_BASIC_TLVS;/dell-lldp:mgmt-address-enable=false"
                app_common_yang="&LLDP_RANGE_INTF_NAME;"
                />
            <PARAM
                name="port-description"
                help="Enable port description tlv"
                ptype="SUBCOMMAND"
                mode="subcommand"
                sub_oper_yang="arg1:port-desc-enable"
                yang_name="&LLDP_RANGE_INTF_NAME;,&LLDP_RANGE_BASIC_TLVS;/dell-lldp:port-desc-enable=false"
                app_common_yang="&LLDP_RANGE_INTF_NAME;"
                />
            <PARAM
                name="system-capabilities"
                help="Enable system capabilities tlv"
                ptype="SUBCOMMAND"
                mode="subcommand"
                sub_oper_yang="arg1:sys-cap-enable"
                yang_name="&LLDP_RANGE_INTF_NAME;,&LLDP_RANGE_BASIC_TLVS;/dell-lldp:sys-cap-enable=false"
                app_common_yang="&LLDP_RANGE_INTF_NAME;"
                />
            <PARAM
                name="system-description"
                help="Enable system description tlv"
                ptype="SUBCOMMAND"
                mode="subcommand"
                sub_oper_yang="arg1:sys-desc-enable"
                yang_name="&LLDP_RANGE_INTF_NAME;,&LLDP_RANGE_BASIC_TLVS;/dell-lldp:sys-desc-enable=false"
                app_common_yang="&LLDP_RANGE_INTF_NAME;"
                />
            <PARAM
                name="system-name"
                help="Enable system name tlv"
                ptype="SUBCOMMAND"
                mode="subcommand"
                sub_oper_yang="arg1:sys-name-enable"
                yang_name="&LLDP_RANGE_INTF_NAME;,&LLDP_RANGE_BASIC_TLVS;/dell-lldp:sys-name-enable=false"
                app_common_yang="&LLDP_RANGE_INTF_NAME;"
                />
        </PARAM>
        <PARAM
            name="management-address"
            test="'${basic-tlv-subcmds}' != 'management-address'"
            help="Enable management address tlv"
            ptype="SUBCOMMAND"
            mode="subcommand"
            optional="true"
            sub_oper_yang="arg1:mgmt-address-enable"
            yang_name="&LLDP_RANGE_BASIC_TLVS;/dell-lldp:mgmt-address-enable=false"
            app_common_yang="&LLDP_RANGE_INTF_NAME;"
            />
        <PARAM
