<ROOT>
  <MACRODEF name="IPV6-PREFIX-SUB-COMMANDS">
    <PARAM
         help="Do not advertise prefix"
         name="no-advertise"
         optional="true"
         mode="subcommand"
         ptype="SUBCOMMAND"
         yang_name="arg1/dell-ip:advertise=false"/>
    <!---  ipv6 nd prefix {ipv6-prefix | default } [no-autoconfig] -->
    <PARAM
        help="Do not use prefix for autoconfiguration"
        name="no-autoconfig"
        optional="true"
        mode="subcommand"
        ptype="SUBCOMMAND"
        yang_name="arg1/dell-ip:autoconfig=false"/>

    <!--- ipv6 nd prefix {ipv6-prefix | default } [no-rtn-address] -->
    <PARAM
        help="Do not send full router address in prefix advert"
        name="no-rtr-address"
        optional="true"
        mode="subcommand"
        ptype="SUBCOMMAND"
        yang_name="arg1/dell-ip:rtr-address=false"/>

    <!--- ipv6 nd prefix {ipv6-prefix | default } [off-link] -->
    <PARAM
        help="Do not use prefix for onlink determination"
        name="off-link"
        optional="true"
        mode="subcommand"
        ptype="SUBCOMMAND"
        yang_name="arg1/dell-ip:off-link=false"/>

    <!--- ipv6 nd prefix {ipv6-prefix | default } [lifetime { valid | infinite} {preferred } infinite}] -->
    <PARAM
        help="Prefix lifetime"
        name="lifetime"
        optional="true"
        mode="subcommand"
        ptype="SUBCOMMAND">
            <PARAM
                help="Select valid or infinite"
                name="valid-lifetime-type"
                mode="switch"
                ptype="SUBCOMMAND">
                    <PARAM
                        help="Valid Lifetime (secs)"
                        name="valid"
                        ptype="RANGE_0_4294967295"
                        yang_name="arg1/dell-ip:lifetime/dell-ip:valid-range=${valid}">
                        <PARAM
                            help="Select preferred or infinite"
                            name="pref-lifetime-type"
                            mode="switch"
                            ptype="SUBCOMMAND">
                            <PARAM
                                help="Preferred Lifetime (secs)"
                                name="preferred"
                                ptype="RANGE_0_4294967295"
                                yang_name="arg1/dell-ip:lifetime/dell-ip:pref-range=${preferred}"/>
                            <PARAM
                                help="Infinite preferred lifetime (secs) "
                                name="infinite"
                                mode="subcommand"
                                ptype="SUBCOMMAND"
                                yang_name="arg1/dell-ip:lifetime/dell-ip:pref-infinite=infinite"/>
                        </PARAM>
                    </PARAM>
                    <PARAM
                        help="Infinite valid lifetime (secs) "
                        name="infinite"
                        mode="subcommand"
                        ptype="SUBCOMMAND"
                        yang_name="arg1/dell-ip:lifetime/dell-ip:valid-infinite=infinite">
                        <PARAM
                            help="Select preferred or infinite"
                            name="pref-lifetime-type"
                            mode="switch"
                            ptype="SUBCOMMAND">
                            <PARAM
                                help="Preferred Lifetime (secs)"
                                name="preferred"
                                ptype="RANGE_0_4294967295"
                                yang_name="arg1/dell-ip:lifetime/dell-ip:pref-range=${preferred}"/>
                            <PARAM
                                help="Infinite preferred lifetime (secs) "
                                name="infinite"
                                mode="subcommand"
                                ptype="SUBCOMMAND"
                                yang_name="arg1/dell-ip:lifetime/dell-ip:pref-infinite=infinite"/>
                        </PARAM>
                    </PARAM>
            </PARAM>
    </PARAM>
  </MACRODEF>
</ROOT>
