<MACRODEF name="INTF_CHANNEL_GROUP">
    <COMMAND
        name="channel-group"
        help="Configure port channel parameters"
        operation="edit-config"
        namespace="&MODULE-NS-IETF-IF;"
        default_oper="none"
        yang_key="&CMS_INTERNAL_IF_ENTRY;/name;&CMS_INTERNAL_IF_ENTRY;/name=&DN_IF_NAME_ONLY;"
        pfeature="FEATURE_CONFIG_OS10_STYLE"
        >
        <!-- channel-group <number> -->
        <PARAM
            name="port-channel_num"
            help="Specify a port channel number"
            ptype="LAG_NUM"
            db_multi_yang_name="&CMS_INTERNAL_IF_ENTRY;/lag-member"
            sub_oper_yang="merge:lag-mode,merge:member-ports"
            yang_name="&DN_IF_ENTRY;/name=port-channel${port-channel_num},&DN_IF_ENTRY;/&MODULE-PREF-IF;:lag-mode=STATIC,&DN_IF_ENTRY;/&MODULE-PREF-IF;:arg1/&MODULE-PREF-IF;:name=${ifacename}">
            <!-- channel-group <number> mode [active | on | passive] -->
            <PARAM
                name="mode"
                help="Set the LACP actor mode"
                ptype="SUBCOMMAND"
                mode="subcommand"
                optional="true">
                <PARAM
                    name="portchannel-mode-switch"
                    help="Specify channeling mode"
                    ptype="SUBCOMMAND"
                    mode="switch"
                    >
                    <PARAM
                        name="active"
                        help="Set channeling mode to ACTIVE"
                        ptype="SUBCOMMAND"
                        mode="subcommand"
                        prompt_fn="replace_yang_name_substring"
                        prompt_oper="STATIC"
                        prompt_str="DYNAMIC"
                        db_multi_yang_name="&CMS_INTERNAL_IF_ENTRY;/lag-mode=lacp-dynamic"
                        sub_oper_yang="merge:lag-mode,merge:member-ports,merge:lacp-mode"
                        yang_name="&DN_IF_ENTRY;/&MODULE-PREF-IF;:arg1/&MODULE-PREF-IF;:lacp-mode=ACTIVE"/>
                    <PARAM
                        name="on"
                        help="Set channeling mode to static"
                        ptype="SUBCOMMAND"
                        db_multi_yang_name="&CMS_INTERNAL_IF_ENTRY;/lag-mode=static"
                        mode="subcommand"/>
                    <PARAM
                        name="passive"
                        help="Set channeling mode to PASSIVE"
                        ptype="SUBCOMMAND"
                        mode="subcommand"
                        prompt_fn="replace_yang_name_substring"
                        prompt_oper="STATIC"
                        prompt_str="DYNAMIC"
                        db_multi_yang_name="&CMS_INTERNAL_IF_ENTRY;/lag-mode=lacp-manual"
                        yang_name="&DN_IF_ENTRY;/&MODULE-PREF-IF;:arg1/&MODULE-PREF-IF;:lacp-mode=PASSIVE"/>
                </PARAM> <!-- portchannel-mode-switch -->
            </PARAM> <!-- mode -->
        </PARAM> <!-- port-channel ... -->
        <ACTION
            builtin="clish_extn"
            />
    </COMMAND>
    <COMMAND
        name="no channel-group"
        help="Configure port channel parameters"
        operation="edit-config"
        namespace="&MODULE-NS-IETF-IF;,dell-cms-int=http://www.dellemc.com/networking/os10/dell-cms-internal"
        prompt_fn="insert_channel_group_name"
        prompt_oper="/dell-cms-int:cms-interface-backptr/dell-cms-int:interface-in-candidate/dell-cms-int:name=${ifacename}"
        yang_name="&DN_IF_ENTRY;/name=,&DN_IF_ENTRY;/&MODULE-PREF-IF;:arg1/&MODULE-PREF-IF;:name=${ifacename}"
        sub_oper_yang="arg2:member-ports"
        pfeature="FEATURE_CONFIG_OS10_STYLE"
        >
        <ACTION
            builtin="clish_extn"
            />
    </COMMAND>
</MACRODEF>
