        <PARAM
            name="mode to choose"
            help="Bind mode to choose"
            ptype="SUBCOMMAND"
            mode="switch" >

            <PARAM
                name="auto"
                help="Automatic settings(default)"
                ptype="SUBCOMMAND"
                db_flag="SKIP_TREE"
                mode="subcommand"
                yang_name= "arg2,arg3/dell-if:negotiation=auto">
